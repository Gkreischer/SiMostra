{
  "name": "orcamentos",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true
  },
  "properties": {
    "nome": {
      "type": "string",
      "required": true
    },
    "cpfcnpj": {
      "type": "number",
      "required": true
    },
    "telefone": {
      "type": "number",
      "required": true
    },
    "endereco": {
      "type": "string",
      "required": true
    },
    "bairro": {
      "type": "string",
      "required": true
    },
    "cidade": {
      "type": "string",
      "required": true
    },
    "cep": {
      "type": "number",
      "required": true
    },
    "formaPagamento": {
      "type": "string",
      "required": true
    },
    "valorPago": {
      "type": "number",
      "required": true
    },
    "dataEntrega": {
      "type": "string",
      "required": true
    },
    "parcelado": {
      "type": "boolean",
      "default": "false"
    },
    "numeroParcelas": {
      "type": "number",
      "default": 0
    },
    "precoTotal": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "observacao": {
      "type": "string"
    },
    "pecasOrcamento": {
      "type": [
        "object"
      ],
      "required": true
    },
    "situacao": {
      "type": "string",
      "required": "true",
      "default": "A PAGAR"
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {}
}
