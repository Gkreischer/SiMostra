{
  "name": "contato",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true
  },
  "properties": {
    "nome": {
      "type": "string",
      "required": true
    },
    "telefone": {
      "type": "number",
      "required": true
    },
    "email": {
      "type": "string",
      "required": true
    },
    "mensagem": {
      "type": "string",
      "required": true
    },
    "situacao": {
      "type": "string",
      "default": "aguardando resposta"
    }
  },
  "validations": [],
  "relations": {},
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
